RewriteEngine on

RewriteRule ^home$ media.php?module=home [L]
RewriteRule ^gallery(.*)\.html$ media.php?module=gallery&id=$1 [L]
RewriteRule ^cara-pembelian\.html$ media.php?module=carabeli&id=$1 [L]
RewriteRule ^login\.html$ media.php?module=login&id=$1 [L]
RewriteRule ^produk-(.*)\.html$ media.php?module=detailproduk&id=$1 [L]
RewriteRule ^kategori-(.*)\.html$ media.php?module=detailkategori&id=$1 [L]
RewriteRule ^halkategori-([0-9]+)-(.*)\.html$ media.php?module=detailkategori&id=$1&halkategori=$2 [L]
RewriteRule ^profil-kami\.html$ media.php?module=profilkami&id=$1 [L]
RewriteRule ^hubungi-kami\.html$ media.php?module=hubungikami&id=$1 [L]
RewriteRule ^hubungi-aksi\.html$ media.php?module=hubungiaksi&id=$1 [L]
RewriteRule ^semua-berita\.html$ media.php?module=semuaberita&id=$1 [L]
RewriteRule ^berita-(.*)\.html$ media.php?module=detailberita&id=$1 [L]
RewriteRule ^halberita-(.*)\.html$ media.php?module=semuaberita&halberita=$1 [L]
RewriteRule ^analisa-kepuasan\.html$ media.php?module=analisa&id=$1 [L]
RewriteRule ^hasil-analisa\.html$ media.php?module=hasil&id=$1 [L]
RewriteRule ^hasil-akhir\.html$ media.php?module=hasilakhir&id=$1 [L]
RewriteRule ^analisa1\.html$ media.php?module=analisa1&id=$1 [L]
RewriteRule ^analisa2\.html$ media.php?module=analisa2&id=$1 [L]
RewriteRule ^simpan-analisa\.html$ media.php?module=simpan&id=$1 [L]

Options All -Indexes
